{
  "version": 2,
  "name": "bee-toxicity-api",
  "builds": [
    {
      "src": "app/backend/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/backend/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  }
}

